{
    "$id": "https://example.com/address.schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Grafana parameters",
    "type": "object",
    "properties": {
      "users": {
        "type": "object",
        "properties": {
          "default": {
            "type": "object",
            "properties": {
              "theme": {
                "type": "string",
                "description": "Specifies the background colour of the Grafana UI. You can change it to 'dark' or 'light'",
                "default": "light",
                "examples": ["light", "dark"]
              }
            }
          }
        }
      }
    }
}
