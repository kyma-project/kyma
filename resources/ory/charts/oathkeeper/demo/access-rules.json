[
  {
    "id": "rule-1",
    "upstream": {
      "url": "https://httpbin.org/anything"
    },
    "match": {
      "url": "http://<[^/]+>/authenticator/noop/authorizer/allow/mutator/noop",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH"
      ]
    },
    "authenticators": [
      {
        "handler": "noop"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "mutators": [{
      "handler": "noop"
    }]
  },
  {
    "id": "rule-2",
    "upstream": {
      "url": "https://httpbin.org/anything"
    },
    "match": {
      "url": "http://<[^/]+>/authenticator/anonymous/authorizer/allow/mutator/header",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH"
      ]
    },
    "authenticators": [
      {
        "handler": "anonymous"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "mutators": [{
      "handler": "header"
    }]
  },
  {
    "id": "rule-3",
    "upstream": {
      "url": "https://httpbin.org/anything"
    },
    "match": {
      "url": "http://<[^/]+>/authenticator/anonymous/authorizer/allow/mutator/id_token",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH"
      ]
    },
    "authenticators": [
      {
        "handler": "anonymous"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "mutators": [{
      "handler": "id_token"
    }]
  }
]
